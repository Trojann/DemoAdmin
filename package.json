    "build": "npm-run-all --parallel build:client build:server",
    "start:server": "cross-env NODE_ENV=localhost PORT=8080 babel-node src/server",
    "start:webpackdev": "webpack-dev-server --config webpack/webpack.dev.config.js",
    "start": "npm-run-all --parallel start:server start:webpackdev"
  },
