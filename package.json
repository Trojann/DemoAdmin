    "scripts": {
        "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx . --fix",
        "release": "standard-version-esdoc",
        "doc:api": "./node_modules/.bin/apidoc --i src/server/route/api --o doc/api",
        "doc:component": "./node_modules/.bin/esdoc -c site/component/.esdoc.json",
        "build:client": "rm -rf build/temp/ && webpack --config webpack/webpack.config.js --progress",
        "build:server": "rm -rf dist/ && babel --copy-files src --out-dir dist",
        "build": "npm-run-all --parallel build:client build:server",
        "start:server": "cross-env NODE_ENV=localhost PORT=8080 babel-node src/server",
        "start:webpackdev": "webpack serve --config webpack/webpack.dev.config.js",
        "start": "npm-run-all --parallel start:server start:webpackdev"
    },
